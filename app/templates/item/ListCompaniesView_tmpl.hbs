{{!empty ListcompaniesviewTmpl template}}

<h4>Compañías</h4>

<table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>¿Está activa?</th>
    </tr>
  </thead>
  {{#each items}}
    <tr>
      <td><a href="#verCompania/{{id}}">{{name}}</a></td>
      <td>{{#if is_active}}Sí{{else}}No{{/if}}</td>
    </tr>
  {{/each}}
</table>
